{
    "env": {
        "es2021": true,
        "node": true
    },
    "overrides": [],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/no-non-null-assertion": "off"
    },
    "plugins": [
        "prettier",
        "@typescript-eslint"
    ],
    "ignorePatterns": [
        "node_modules",
        "dist"
    ]
}